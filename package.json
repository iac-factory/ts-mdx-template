{
    "name": "ts-mdx-template",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@mdx-js/loader": "^2.1.4",
        "@mdx-js/react": "^2.1.4",
        "classnames": "^2.3.2",
        "highlight.js": "^11.6.0",
        "react": "^18.2.0",
        "react-app-polyfill": "^3.0.0",
        "react-dom": "^18.2.0",
        "react-grid-system": "^8.1.6",
        "react-modal": "^3.15.1",
        "react-refresh": "^0.14.0",
        "react-router-dom": "^6.4.2",
        "web-vitals": "^3.0.3"
    },
    "scripts": {
        "prestart": "npm run spline",
        "start": "node scripts/start.js",
        "build": "node scripts/build.js",
        "test": "node scripts/test.js",
        "spline": "ts-node spline -- --source blog"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "settings": {
            "extends": "@iac-factory/eslint-settings"
        },
        "parserOptions": {
            "ecmaVersion": "latest",
            "sourceType": "module"
        },
        "rules": {
            "quotes": [
                "error",
                "double",
                {
                    "allowTemplateLiterals": true
                }
            ],
            "indent": [
                "error",
                4
            ]
        }
    },
    "packageManager": "npm",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.16.0",
        "@babel/plugin-syntax-import-assertions": "^7.18.6",
        "@iac-factory/eslint-settings": "^1.7.36",
