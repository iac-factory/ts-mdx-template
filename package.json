        "postcss-preset-env": "^7.0.1",
        "prompts": "^2.4.2",
        "remark": "^14.0.2",
        "remark-frontmatter": "^4.0.1",
        "remark-gfm": "^3.0.1",
        "remark-mdx-code-meta": "^2.0.0",
        "remark-mdx-frontmatter": "^2.0.3",
        "remark-toc": "^8.0.1",
        "resolve": "^1.20.0",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.54.5",
        "sass-loader": "^13.0.2",
        "semver": "^7.3.5",
        "source-map-loader": "^4.0.0",
        "style-loader": "^3.3.1",
        "terser-webpack-plugin": "^5.2.5",
        "ts-loader": "^9.3.1",
        "ts-node-dev": "^2.0.0",
